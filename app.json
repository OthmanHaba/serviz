{
  "expo": {
    "name": "Serviz",
    "slug": "Serviz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/serv_logo.jpg",
    "scheme": "serviz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.othmanhaba.Serviz",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show your position on the map"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDCXFvA0zePher8Vvtd14UQLByfk5-VGG4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/serv_logo.jpg",
        "backgroundColor": "#ffffff"
      },

      "package": "com.othmanhaba.Serviz",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDCXFvA0zePher8Vvtd14UQLByfk5-VGG4"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/serv_logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "693176c4-11a6-40ae-8ed1-62363ce64307"
      }
    }
  }
}
